\begin_layout Subsubsection

\lang english
Discount rates for Non-Financial Liabilities
\begin_inset CommandInset label
LatexCommand label
name "subsubsec:10.3.5.1_Discount_rates"

\end_inset


\end_layout

\begin_layout Standard

\lang english
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
stepcounter{SubSubSecCounter}
\end_layout

\end_inset

 
\end_layout

\begin_layout Standard

