\begin_layout Subsubsection
Затратный подход
\begin_inset CommandInset label
LatexCommand label
name "subsubsec:5.3.7.3_Cost_approach"

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


