\begin_layout Paragraph

\lang english
Fair Value
\begin_inset CommandInset label
LatexCommand label
name "par:9.4.2.2.1_Fair_Value_IFRS"

\end_inset


\end_layout

\begin_layout Standard

\lang english
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
stepcounter{ParCounter}
\end_layout

\end_inset

 
\end_layout

\begin_layout Standard

\lang english
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
theparagraph.
\backslash
theParCounter.
\end_layout

\end_inset


\begin_inset CommandInset label
LatexCommand label
name "9.4.2.2.1.1"

\end_inset

 IFRS 13 defines Fair Value as the price that would be received to sell
 an asset or paid to transfer a liability in an orderly transaction between
 market participants at the measurement date.
\begin_inset CommandInset label
LatexCommand label
name "9.4.2.2.1.1-End"

\end_inset


\end_layout

