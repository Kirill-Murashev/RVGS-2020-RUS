\begin_inset CommandInset label
LatexCommand label
name "4.1.3.13.1"

\end_inset


\emph on
 Оценщик
\emph default
 обязан привести сведения о
\begin_inset space ~
\end_inset

разрешённом использовании и
\begin_inset space ~
\end_inset

возможности обнародования и
\begin_inset space ~
\end_inset

распространения 
\emph on
отчёта об
\begin_inset space ~
\end_inset

оценке
\emph default
.
\begin_inset CommandInset label
LatexCommand label
name "4.1.3.13.1-End"

\end_inset


\end_layout

