\begin_inset CommandInset label
LatexCommand label
name "5.3.8.2"

\end_inset

 Большинство 
\emph on
отчётов об
\begin_inset space ~
\end_inset

оценке
\emph default
 бизнеса содержат следующие разделы, порядок которых может отличаться от
\begin_inset space ~
\end_inset

приведённого ниже
\end_layout

\begin_layout Itemize
введение;
\end_layout

\begin_layout Itemize

\emph on
цель оценки
\emph default
 и
\begin_inset space ~
\end_inset


\shape slanted
вид определяемой стоимости
\shape default
;
\end_layout

\begin_layout Itemize

\shape slanted
допущения
\shape default
 и
\begin_inset space ~
\end_inset


\shape slanted
специальные допущения
\shape default
;
\end_layout

\begin_layout Itemize
описание 
\emph on
объекта оценки
\emph default
;
\end_layout

\begin_layout Itemize
описание и
\begin_inset space ~
\end_inset

история оцениваемого предприятия
\begin_inset space ~
\end_inset

(бизнеса);
\end_layout

\begin_layout Itemize
бухгалтерская отчётность и
\begin_inset space ~
\end_inset

учётная политика предприятия;
\end_layout

\begin_layout Itemize
анализ 
\shape slanted
финансовой отчётности
\shape default
;
\end_layout

\begin_layout Itemize
анализ деловых и
\begin_inset space ~
\end_inset

маркетинговых планов и
\begin_inset space ~
\end_inset

прогноз развития;
\end_layout

\begin_layout Itemize
выводы по
\begin_inset space ~
\end_inset

результатам анализа сопоставимых компаний и
\begin_inset space ~
\end_inset

сделок;
\end_layout

\begin_layout Itemize
описание отрасли
\begin_inset space ~
\end_inset

(отраслей), в
\begin_inset space ~
\end_inset

которой
\begin_inset space ~
\end_inset

(которых) осуществляет свою деятельность оцениваемая компания, общей экономическ
ой ситуации, доходности и
\begin_inset space ~
\end_inset

рисков;
\end_layout

\begin_layout Itemize
экологические ограничения;
\end_layout

\begin_layout Itemize

\emph on
методы оценки
\emph default
 и
\begin_inset space ~
\end_inset

