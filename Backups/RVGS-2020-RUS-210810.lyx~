\begin_inset CommandInset label
LatexCommand label
name "5.5.4.2"

\end_inset

 Ниже приводятся примеры 
\emph on
основных средств
\emph default
:
\end_layout

\begin_layout Itemize
технологические либо производственные установки и
\begin_inset space ~
\end_inset

оборудование;
\end_layout

\begin_layout Itemize
объекты транспортной инфраструктуры;
\end_layout

\begin_layout Itemize
разведанные запасы полезных ископаемых, шахты и
\begin_inset space ~
\end_inset

металлы;
\end_layout

\begin_layout Itemize
автотранспорт, железнодорожный подвижной состав, морские и
\begin_inset space ~
\end_inset

воздушные суда;
\end_layout

\begin_layout Itemize
компьютерное оборудование, оргтехника и
\begin_inset space ~
\end_inset

офисная мебель;
\end_layout

\begin_layout Itemize
передвижные установки;
\end_layout

\begin_layout Itemize
общезаводское оборудование;
\end_layout

\begin_layout Itemize
вспомогательное оборудование.
\begin_inset Foot
status open

\begin_layout Plain Layout
Прим.
 пер.: в
\begin_inset space ~
\end_inset

ориг.
 англ.
 тексте присутствует ещё
\begin_inset space ~
\end_inset

один пункт 
\begin_inset Quotes rld
\end_inset


\lang english
health, education and defense
\lang russian

\begin_inset Quotes rrd
\end_inset

, что, по
\begin_inset space ~
\end_inset

мнению переводчика, является опечаткой и
\begin_inset space ~
\end_inset

не
\begin_inset space ~
\end_inset

подлежит переводу.
\end_layout

\end_inset


\begin_inset CommandInset label
LatexCommand label
name "5.5.4.2-End"

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


