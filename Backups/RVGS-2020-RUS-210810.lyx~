\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
stepcounter{SubSubSecCounter}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
thesubsection.
\backslash
theSubSubSecCounter.
\end_layout

