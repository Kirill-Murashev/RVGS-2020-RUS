\begin_layout Subsubsection
Коммерческий потенциал
\begin_inset CommandInset label
LatexCommand label
name "subsubsec:5.4.2.12_Trading_potential"

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
