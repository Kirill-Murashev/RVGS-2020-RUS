\begin_inset CommandInset label
LatexCommand label
name "5.3.3.2"

\end_inset

 Важно, чтобы 
\emph on
задание на
\begin_inset space ~
\end_inset

оценку
\emph default
 и
\begin_inset space ~
\end_inset


\shape slanted
условия договора на
\begin_inset space ~
\end_inset

проведение оценки
\shape default
 были понятны и
\begin_inset space ~
\end_inset

согласованы между 
\emph on
оценщиком
\emph default
 и
\begin_inset space ~
\end_inset

заказчиком до
\begin_inset space ~
\end_inset

начала выполнения работы.
 Актив или
\begin_inset space ~
\end_inset

обязательство, конкретная доля в
\begin_inset space ~
\end_inset

активе или
\begin_inset space ~
\end_inset

обязательстве либо подлежащие 
\shape slanted
оценке
\shape default
 права на
\begin_inset space ~
\end_inset

них должны быть зафиксированы в
\begin_inset space ~
\end_inset

письменном виде.
 Их
\begin_inset space ~
\end_inset

описание должно содержать следующие сведения:
\end_layout

\begin_layout Itemize
организационно-правовая форма бизнеса;
\end_layout

\begin_layout Itemize
размер оцениваемой доли в
\begin_inset space ~
\end_inset

активе и
\begin_inset space ~
\end_inset

её
\begin_inset space ~
\end_inset

статус;
\end_layout

\begin_layout Itemize
состав 
\emph on
объекта оценки
\emph default
, а
\begin_inset space ~
\end_inset

также выведенные за
\begin_inset space ~
\end_inset

её
\begin_inset space ~
\end_inset

периметр активы и
\begin_inset space ~
\end_inset

обязательства;
\end_layout

\begin_layout Itemize
тип
\begin_inset space ~
\end_inset

оцениваемых акций либо вид
\begin_inset space ~
\end_inset

оцениваемой доли.
\begin_inset CommandInset label
LatexCommand label
name "5.3.3.2-End"

\end_inset


\end_layout

