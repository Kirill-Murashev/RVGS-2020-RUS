\begin_inset CommandInset label
LatexCommand label
name "4.4.6.3"

\end_inset

 При
\begin_inset space ~
\end_inset

проведении 
\shape slanted
оценки
\shape default
 
\shape slanted
справедливой стоимости
\shape default
 от
\begin_inset space ~
\end_inset


\emph on
оценщика
\emph default
 требуется определить всё
\begin_inset space ~
\end_inset

нижеперечисленное:
\end_layout

\begin_layout Itemize
конкретный актив или
\begin_inset space ~
\end_inset

обязательство, являющийся
\begin_inset space ~
\end_inset

(являющееся) 
\emph on
объектом оценки
\emph default
, а
\begin_inset space ~
\end_inset

также соответствующую расчётную единицу;
\end_layout

\begin_layout Itemize
для
\begin_inset space ~
\end_inset

нефинансовых активов 
\begin_inset ERT
status open

\begin_layout Plain Layout

\begin_inset Quotes rld
\end_inset

---
\end_layout

\end_inset

 соответствующие 
\emph on
предпосылки оценки
\emph default
 в
\begin_inset space ~
\end_inset

соответствии с
\begin_inset space ~
\end_inset

их
\begin_inset space ~
\end_inset

наилучшим и
\begin_inset space ~
\end_inset

наиболее эффективным использованием;
\end_layout

\begin_layout Itemize
основной либо наиболее выгодный рынок для
\begin_inset space ~
\end_inset

актива или
\begin_inset space ~
\end_inset

обязательства;
\end_layout

\begin_layout Itemize
применимый(е) с
\begin_inset space ~
\end_inset

учётом наличия исходных данных метод(ы) 
\shape slanted
оценки
\shape default
, на
\begin_inset space ~
\end_inset

основе которого
\begin_inset space ~
\end_inset

(которых) можно разработать вводные, реализованные в
\begin_inset space ~
\end_inset

форме 
\shape slanted
допущений
\shape default
, которые рыночные агенты использовали
\begin_inset space ~
\end_inset

бы при
\begin_inset space ~
\end_inset

определении цены актива или
\begin_inset space ~
\end_inset

обязательства, а
\begin_inset space ~
\end_inset

также степень упорядочивания 
\shape slanted
справедливой стоимости
\shape default
, в
\begin_inset space ~
\end_inset

соответствии с
\begin_inset space ~
\end_inset

которой проводится классификация исходных данных.
\begin_inset CommandInset label
LatexCommand label
name "4.4.6.3-End"

\end_inset


\end_layout

