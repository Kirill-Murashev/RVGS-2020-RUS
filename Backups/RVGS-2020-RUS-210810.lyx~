\begin_layout Subsection

\lang english
Valuation Approaches and Methods
\begin_inset CommandInset label
LatexCommand label
name "subsec:10.3.4_Valuation_Approaches"

\end_inset


\end_layout

\begin_layout Standard

\lang english
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
stepcounter{SubSubSecCounter}
\end_layout

\end_inset

 
\end_layout

\begin_layout Standard

\lang english
\begin_inset ERT
