key "IVS-2020"
literal "false"

\end_inset

, к
\begin_inset space ~
\end_inset

которым 
\emph on
оценщикам
\emph default
 следует обращаться по
\begin_inset space ~
\end_inset

мере необходимости.
\end_layout

