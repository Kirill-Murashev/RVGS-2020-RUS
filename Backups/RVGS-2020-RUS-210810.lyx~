\begin_layout Subsection

\lang english
Introduction
\begin_inset CommandInset label
LatexCommand label
name "subsec:10.3.2_Introduction"

\end_inset


\end_layout

\begin_layout Standard

\lang english
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
stepcounter{SubSecCounter}
\end_layout

\end_inset

 
\end_layout

\begin_layout Standard

\lang english
\begin_inset ERT
