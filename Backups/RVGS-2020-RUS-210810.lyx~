\begin_layout Subsubsection

\lang english
Restrictions on transfers
\begin_inset CommandInset label
LatexCommand label
name "subsubsec:10.3.5.3_Restrictions_on_transfers"

\end_inset


\end_layout

\begin_layout Standard

\lang english
\begin_inset ERT
status open

