\begin_layout Standard

\series bold
Характер и
\begin_inset space ~
\end_inset

источники соответствующих информации и
\begin_inset space ~
\end_inset

данных, а
\begin_inset space ~
\end_inset

также степень их
\begin_inset space ~
\end_inset

верификации, осуществляемой при
\begin_inset space ~
\end_inset

проведении 
\shape slanted
оценки
\shape default
, необходимо определить и
\begin_inset space ~
\end_inset

описать в
\begin_inset space ~
\end_inset


\shape slanted
договоре на
\begin_inset space ~
\end_inset

проведение оценки
\shape default
.
 В
\begin_inset space ~
\end_inset

целях данного раздела под
\begin_inset space ~
\end_inset


\emph on
информацией
\emph default
 следует понимать 
\emph on
исходные данные
\emph default
 и
\begin_inset space ~
\end_inset

иные подобные вводные.
\begin_inset Foot
status open

\begin_layout Plain Layout
Прим.
\begin_inset space ~
\end_inset

пер.: таким образом, в
\begin_inset space ~
\end_inset

контексте данного подраздела происходит смешение понятий 
\emph on

\begin_inset Quotes rld
\end_inset

информация
\begin_inset Quotes rrd
\end_inset


\emph default
 и
\begin_inset space ~
\end_inset


