\begin_layout Section

\lang english
IVS 210.
 Intangible Assets
\begin_inset CommandInset label
LatexCommand label
name "sec:10.2_IVS-210_Intangible_Assets"

\end_inset


\end_layout

\begin_layout Subsection

\lang english
Overview
\begin_inset CommandInset label
LatexCommand label
name "subsec:10.2.1_Overview"

\end_inset


\end_layout

\begin_layout Standard

\lang english
\begin_inset ERT
