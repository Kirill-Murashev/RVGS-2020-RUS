\begin_layout Subsection

\lang english
Bases of Value
\begin_inset CommandInset label
LatexCommand label
name "subsec:10.5.3_Bases_of_Value"

\end_inset


\end_layout

\begin_layout Standard

\lang english
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
stepcounter{SubSecCounter}
\end_layout

\end_inset

 
\end_layout

